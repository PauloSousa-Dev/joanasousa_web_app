{
  "name": "joanasousa_web_app",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently -k -n CL,NEXT \"contentlayer dev\" \"next dev\"",
    "build": "contentlayer build && next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "contentlayer": "0.3.4",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
